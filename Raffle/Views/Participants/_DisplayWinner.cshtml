@model IEnumerable<Raffle.Models.Winners>



<div id="wrapper">
    <br />
    @foreach (var w in Model)
    { 
        <div class="text-center customFont">
            <h5>
                <b><span class="winnerText">@w.winner_name</span></b>
                <br />
                @w.winner_department
                <br />
                @w.prize_name
            </h5>
        </div>
        <br />
    }
</div>